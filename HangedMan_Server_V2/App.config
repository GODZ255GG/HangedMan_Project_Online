<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<bindings>
			<netTcpBinding>
				<binding>
					<security mode="None">
						<transport sslProtocols="None" />
					</security>
				</binding>
			</netTcpBinding>
		</bindings>
		<behaviors>
			<serviceBehaviors>
				<behavior name="">
					<dataContractSerializer maxItemsInObjectGraph="6553500" />
					<serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service name="Services.HangedManService">
				<endpoint address="Services" binding="netTcpBinding" contract="Services.IPlayerServices"/>
				<endpoint address="Services" binding="netTcpBinding" contract="Services.IGameServices"/>
				<endpoint address="Services" binding="netTcpBinding" contract="Services.IWordServices"/>
				<host>
					<baseAddresses>
						<add baseAddress="http://192.168.1.69:8082/" />
						<add baseAddress="net.tcp://192.168.1.69:8081/" />
					</baseAddresses>
				</host>
			</service>
		</services>
	</system.serviceModel>
</configuration>